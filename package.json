  "files": [
    "build"
  ],
  "homepage": "https://github.com/stackcache/vebug#readme",
  "keywords": [
    "react",
    "debug",
    "css",
    "visual",
    "typescript",
    "component"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@types/styled-components": "^4.1.19",
